extends ../jade/_base_dashboard

block variables
  -var STATIC_URL = "../../../static/"
  -var src_img = "dashboard/account_profil"

block content
  section.tabs-vertical-active.sm.scrollbar-custom.scrollbar-left: ul
    -each tab in account_tabs
      li.tab-vertical
        a(href="#{tab.href}.html")
        h3 #{tab.title}
  block sections