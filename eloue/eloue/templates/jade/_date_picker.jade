-var items = ["Du", "Au"]
each item in items
	.row.date-picker
		.table-cell.main-cell
			.input-group
				.input-group-addon #{item}
				input(type="text" placeholder="").form-control
				.input-group-addon.calendar
					span
		.table-cell
			select.form-control
				-for(var i=0;i<24;i++)
					-var n = i>9? i + ":00" : "0" + i + ":00"
					option(value=n) #{n}