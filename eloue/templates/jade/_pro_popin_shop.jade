.modal#pro-popin-shop(tabindex="-1", role="dialog", aria-hidden="true")
	-var is_agencies = false
	.modal-dialog.large
		.modal-content
			.modal-header
				button(type="button", data-dismiss="modal").close
					span(aria-hidden="true")&times;
					span.sr-only Close
				h4.modal-title {{ patron.company_name }} - Venez en magasin
			.modal-body.table-cell.text-justify.pro-shop-modal
				//-.static-part.table-cell
				//-	section.product-info
				//-		include pop_up_sections/_pro_product_info
				.change-part.table-cell
					section(data-ng-controller="ProShopCtrl").phone-form
						include pop_up_sections/_shop_form