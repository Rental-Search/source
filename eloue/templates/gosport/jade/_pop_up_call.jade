-load eloue_url

.modal.fade#phoneModal(tabindex="-1", role="dialog", aria-hidden="true")
    .modal-dialog.large
        .modal-content
            .modal-header
                button(type="button", data-dismiss="modal").close
                    span(aria-hidden="true")&times;
                    span.sr-only Close
                h4.modal-title Appel
            .modal-body.table-cell
                .static-part.table-cell
                    section.product-info
                        include ./pop_up_sections/_product_info.jade
                    section.terms
                        include ./pop_up_sections/_terms
                        a(eloue-dashboard-redirect="{% eloue_url 'dashboard:bookings' %}").btn.bg-success
                            | RÃ©servez
                        a(href="#/message").btn.bg-gray
                            | Envoyez un message
                .change-part.table-cell.top
                    section.table.variant-call
                        include ./pop_up_sections/_variants_call
            .modal-footer
                p Supported by
                    span.s-eloue
