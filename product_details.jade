doctype
html(lang = "en")
	head
		-var title = "e-loue"
		include jade/_head
		
	body
		include jade/_src
		.container-flex
			header
				include jade/_header-site
			nav
				.row-fluid
					include jade/_breadcrumbs

			article.row-fluid
				.container.product-details
					.row.mrg-v-5
						.col-md-8.title
							h3 Location 
								span.name Bmw - 520 touring luxe 
								|- 
								span.location NICE
							ul.breadcrumb
								-var specifications = ["Année 2010", "Moteur Diesel", "Boite Automatique", "10 000 à 30 000 km"]
								-each specification in specifications
									li #{specification}
						.col-md-4.share-btns
					.row
						main.col-md-8
							.row-fluid.framing-v
								include jade/_main
							
						aside.col-md-4
							include jade/_aside

			footer
				.row-fluid.insurances.framing-h
					include jade/_insurances
				.row-fluid.footer-links
					include jade/_footer-links
				.row-fluid.rss.pad-v-4
					include jade/_rss

			include jade/_sign-in
			include jade/_log-in

		include jade/_scripts
